{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "func-names": "off",
        "no-undef": "off",
        "object-shorthand": "off",
        "no-underscore-dangle": "off",
        "no-param-reassign": "off",
        "no-constant-condition": "off",
        "no-case-declarations": "off"
    }
}
